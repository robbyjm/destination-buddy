<mvc:View
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:ce="sap.ui.codeeditor"
  controllerName="sap.ui.demo.basicTemplate.controller.End"
>
  <Page title="Who am I..?">
    <headerContent>
      <Button press='onPress' icon="sap-icon://copy" />
    </headerContent>
    <ce:CodeEditor
      id="code"
      liveChange="prettyPrint"
      type="json"
      colorTheme="clouds"
      editable="false"
      value="{
    path: '/',
    formatter: '.formatter.code'
  }"
    />
  </Page>
</mvc:View>
