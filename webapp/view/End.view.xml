<mvc:View
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:ce="sap.ui.codeeditor"
  controllerName="sap.ui.demo.basicTemplate.controller.End"
>
  <Page title="Who am I..?" >
    <headerContent>
    <Button press='onPress' icon="sap-icon://copy"></Button></headerContent>
    <ce:CodeEditor id="code"
      type="json"
	colorTheme="clouds"
	editable="false" 
  value="{
    path: '/',
    formatter: '.formatter.code'
  }"
  />
  </Page>
</mvc:View>
