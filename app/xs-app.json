{
    "welcomeFile": "/web-pages/hello-world.html",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/web-pages/(.*)$",
            "localDir": "my-static-resources"
        },
        {
            "source": "^/user-api/currentUser$",
            "target": "/currentUser",
            "service": "sap-approuter-userapi"
        },
        {
            "source": "^/user-api/attributes$",
            "target": "/attributes",
            "service": "sap-approuter-userapi"
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "destination": "ui",
            "authenticationType": "xsuaa"
        }
    ]
}